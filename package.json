{
    "name": "blogs",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "next dev --turbopack",
        "build": "next build",
        "start": "next start",
        "lint": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
        "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,scss,css,md}\" \"package.json\" \"tsconfig.json\" \".github/workflows/**/*.yml\"",
        "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,scss,css,md}\" \"package.json\" \"tsconfig.json\" \".github/workflows/**/*.yml\"",
        "prepare": "husky install",
        "test": "jest"
    },
    "lint-staged": {
        "*.{ts,tsx,js,jsx}": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.{scss,css,json,md}": "prettier --write"
    },
    "dependencies": {
        "@hookform/resolvers": "^5.2.0",
        "@reduxjs/toolkit": "^2.8.2",
        "firebase": "^12.0.0",
        "next": "15.4.4",
        "react": "19.1.0",
        "react-dom": "19.1.0",
        "react-hook-form": "^7.61.1",
        "react-redux": "^9.2.0",
        "uuid": "^11.1.0",
        "zod": "^4.0.10"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.3.1",
        "@eslint/js": "^9.31.0",
        "@next/eslint-plugin-next": "^15.4.4",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^16.3.0",
        "@testing-library/user-event": "^14.6.1",
        "@types/jest": "^30.0.0",
        "@types/node": "^20.19.9",
        "@types/react": "^19.1.8",
        "@types/react-dom": "^19.1.6",
        "@typescript-eslint/eslint-plugin": "^8.38.0",
        "@typescript-eslint/parser": "^8.38.0",
        "cross-fetch": "^4.1.0",
        "dotenv-cli": "^9.0.0",
        "eslint": "^9.31.0",
        "eslint-config-next": "15.4.4",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-jest": "^29.0.1",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-prettier": "^5.5.3",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^5.2.0",
        "globals": "^16.3.0",
        "husky": "^9.1.7",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^30.0.5",
        "jest-environment-jsdom": "^30.0.5",
        "lint-staged": "^16.1.2",
        "prettier": "^3.6.2",
        "sass": "^1.89.2",
        "ts-jest": "^29.4.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.8.3"
    }
